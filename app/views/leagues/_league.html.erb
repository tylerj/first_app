<tr>
  <td>
    <%= link_to league.name, league %>
  </td>
  <td>
    type
  </td>
  <td>
    <% if league.password != "" %>
    Yes
    <% else %>
    No
    <% end %>
  </td>
  <td>
    <%= league.admin.name %>
  </td>
  <td>
    <%= league.created_at.strftime("%Y-%m-%d") %>
  </td>
  <td>
  <% if signed_in? %>
    <div id="join_form">
    <% if league.in_league?(current_user) %>
      <%= link_to "Go to League", league_path(league) %>
    <% else %>
      <%= link_to "Join League", league_path(league) %>
    <% end %>
    </div>
  <% end %>    
  </td>
</tr>